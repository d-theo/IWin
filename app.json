{
  "expo": {
    "name": "IWin",
    "slug": "IWin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-trophy.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon-trophy.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF9E1"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.dtheo.iwin",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-trophy.png",
        "backgroundColor": "#FFF9E1"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "775bdd36-eac7-4475-81bd-10e8c95b7cc1"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
